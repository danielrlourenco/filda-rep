{
  "name": "filda-website",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "daniel.r.lourenco <daniel-r-lourenco@telecom.pt>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "build:gh-pages": "DEPLOY_ENV=GH_PAGES nuxt build",
